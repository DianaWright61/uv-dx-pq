    <entry>
        <fullUrl value="https://www.example.org/ObservationDefinition/Particles"/>
        <resource>
            <ObservationDefinition>
                <id value="Particles"/>
                <name value="Particle size distribution"/>
                <status value="active"/>
                <code>
                    <coding> 
                        <system value="http://loinc.org"/>
                        <code value="PART"/>
                        <display value="Particle size distribution"/>
                    </coding>
