                    <range>
                        <high>
                            <value value="145"/>
                            <unit value="µm"/>
                            <system value="http://unitsofmeasure.org"/>
                            <code value="um"/>
                        </high>
                    </range>
                </qualifiedValue>
                <qualifiedValue>
                    <appliesTo> <!-- could be a code -->
                        <text value="D(v,0.1)"/>
                    </appliesTo>
                    <range>
                        <high>
                            <value value="20"/>
                            <unit value="µm"/>
                            <system value="http://unitsofmeasure.org"/>
                            <code value="um"/>
