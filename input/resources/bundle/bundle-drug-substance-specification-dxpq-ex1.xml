    <entry>
        <fullUrl value="https://www.example.org/ObservationDefinition/Assay"/>
        <resource>
            <ObservationDefinition>
                <id value="Assay"/>
                <name value="Assay"/>
                <status value="active"/>
                <code>
                    <coding> 
                        <system value="http://loinc.org"/>
                        <code value="XXX"/>
                        <display value="Assay"/>
                    </coding>
                    <text value="Assay"/>
                </code>
                <method>
                    <text value="Assay by LC"/>
                </method>
                <qualifiedValue>
                    <range>
                        <low>
                            <value value="98"/>
                            <unit value="% w/w"/>
                            <system value="http://unitsofmeasure.org"/>
                            <code value="%{w/w}"/> <!-- not sure this is a correct ucum code -->
                        </low>
                        <high>
                            <value value="102"/>
                            <unit value="% w/w"/>
                            <system value="http://unitsofmeasure.org"/>
                            <code value="%{w/w}"/> <!-- not sure this is a correct ucum code -->
                        </high>
                    </range>
                </qualifiedValue>
            </ObservationDefinition>
        </resource>
    </entry>
    <entry>
        <fullUrl value="https://www.example.org/ObservationDefinition/Impurities"/>
        <resource>
            <ObservationDefinition>
                <id value="Impurities"/>
                <name value="Drug Substance Related Organic impurities"/>
                <status value="active"/>
                <code>
                    <coding> 
                        <system value="http://loinc.org"/>
                        <code value="DGP"/>
                        <display value="Drug Substance Related Organic impurities"/>
                    </coding>
                </code>
                <method>
                    <text value="Organic impurities by LC"/>
                </method>
                <quantitativeDetails>
                    <unit>
                        <coding>
                            <system value="http://unitsofmeasure.org"/>
                            <code value="%{w/w}"/> <!-- not sure this is a correct ucum code -->
                        </coding>
                    </unit>
                    <customaryUnit>
                        <coding>
                            <code value="% w/w"/>
                        </coding>
                    </customaryUnit>
                </quantitativeDetails>
                <qualifiedValue>
                    <appliesTo> <!-- could be a code -->
                        <text value="Impurity 1"/>
                    </appliesTo>
                    <range>
                        <high>
                            <value value="0.5"/>
                            <unit value="% w/w"/>
                        </high>
                    </range>
                </qualifiedValue>
                <qualifiedValue>
                    <appliesTo>
                        <text value="Impurity 2"/>
                    </appliesTo>
                    <range>
                        <high>
                            <value value="0.5"/>
                            <unit value="% w/w"/>
                        </high>
                    </range>
                </qualifiedValue>
                <qualifiedValue>
                    <appliesTo>
                        <text value="Any individual unspecified impurity"/>
                    </appliesTo>
                    <range>
                        <high>
                            <value value="0.10"/>
                            <unit value="% w/w"/>
                        </high>
                    </range>
                </qualifiedValue>
                <qualifiedValue>
                    <appliesTo>
                        <text value="Total impurities"/>
                    </appliesTo>
                    <range>
                        <high>
                            <value value="2.0"/>
                            <unit value="% w/w"/>
                        </high>
                    </range>
                </qualifiedValue>
                <component> <!-- are expected to have a component that says what impurity this one is -->
                    <code>
                        <coding> 
                            <system value="http://loinc.org"/>
                            <code value="IMP"/>
                            <display value="Impurity"/>
                        </coding>
                    </code>
                </component>
            </ObservationDefinition>
        </resource>
    </entry>
    <entry>
        <fullUrl value="https://www.example.org/ObservationDefinition/Mutagenic"/>
        <resource>
            <ObservationDefinition>
                <id value="Mutagenic"/>
                <name value="Mutagenic impurities"/>
                <status value="active"/>
                <code>
                    <coding> 
                        <system value="http://loinc.org"/>
                        <code value="MUT"/>
                        <display value="Mutagenic impurities"/>
                    </coding>
                </code>
                <method>
               <text value="Impurity 5 content by LC-MS"/>
                </method>
                <quantitativeDetails>
                    <unit>
                        <coding>
                            <system value="http://unitsofmeasure.org"/>
                            <code value="ppm"/>
                        </coding>
                    </unit>
                    <customaryUnit>
                        <coding>
                            <code value="ppm"/>
                        </coding>
                    </customaryUnit>
                </quantitativeDetails>
                <qualifiedValue>
                    <appliesTo> <!-- could be a code -->
                        <text value="Impurity - Mutagenic"/>
                    </appliesTo>
                    <range>
                        <high>
                            <value value="50"/>
                            <unit value="ppm"/>
                            <system value="http://unitsofmeasure.org"/>
                            <code value="ppm"/>
                        </high>
                    </range>
                </qualifiedValue>
                <component> <!-- are expected to have a component that says what impurity this one is -->
                    <code>
                        <coding> 
                            <system value="http://loinc.org"/>
