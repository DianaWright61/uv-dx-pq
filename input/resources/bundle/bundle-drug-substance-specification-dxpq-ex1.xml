                        </coding>
                    </unit>
                    <customaryUnit>
                        <coding>
                            <code value="µm"/>
                        </coding>
                    </customaryUnit>
                </quantitativeDetails>
                <qualifiedValue>
                    <appliesTo> <!-- could be a code -->
                        <text value="D(v,0.9)"/>
                    </appliesTo>
                    <range>
                        <high>
                            <value value="319"/>
                            <unit value="µm"/>
                            <system value="http://unitsofmeasure.org"/>
                            <code value="um"/>
                        </high>
                    </range>
                </qualifiedValue>
                <qualifiedValue>
                    <appliesTo> <!-- could be a code -->
